<UserControl x:Class="LEDCube.Simulator.WPF.Views.LEDCubeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LEDCube.Simulator.WPF.Views"
             mc:Ignorable="d" 
             x:Name="rootItem"
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Cube/CubeStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
         <Grid Background="DarkGray"
              x:Name="CubeViewPort3D"
               MouseUp="HandleCubeMouseUp"
              MouseMove="HandleCubeMouseMove"
              MouseWheel="HandleCubeMouseWheel"
              MouseLeave="HandleCubeMouseLeave"
              StylusUp="HandleCubeStylusLeave"
              StylusLeave="HandleCubeStylusLeave">
            <Viewport3D >
                <Viewport3D.Camera>
                    <PerspectiveCamera 
                      Position = "100, 0, 0"
                      LookDirection = "-1, 0, 0"
                      UpDirection = "0, 1, 0"
                      FieldOfView = "90">
                        <PerspectiveCamera.Transform>
                            <Transform3DGroup>
                                <RotateTransform3D>
                                    <RotateTransform3D.Rotation>
                                        <AxisAngleRotation3D
                                          Axis="0 1 0" 
                                          Angle="{Binding ElementName=rootItem, Path=DataContext.HorizontalRotationAngle}" />
                                    </RotateTransform3D.Rotation>
                                </RotateTransform3D>
                                <RotateTransform3D>
                                    <RotateTransform3D.Rotation>
                                        <AxisAngleRotation3D
                                          Axis="1 0 0" 
                                          Angle="{Binding ElementName=rootItem, Path=DataContext.VerticalRotationAngle}" />
                                    </RotateTransform3D.Rotation>
                                </RotateTransform3D>
                                <ScaleTransform3D CenterX="0" CenterY="0" CenterZ="0"
                                                  ScaleX="{Binding ElementName=rootItem, Path=DataContext.ZoomScale}"
                                                  ScaleY="{Binding ElementName=rootItem, Path=DataContext.ZoomScale}"
                                                  ScaleZ="{Binding ElementName=rootItem, Path=DataContext.ZoomScale}"
                                                  >
                                    
                                </ScaleTransform3D>
                            </Transform3DGroup>
                        </PerspectiveCamera.Transform>
                    </PerspectiveCamera>
                </Viewport3D.Camera>
                <ModelVisual3D>
                    <ModelVisual3D.Content>
                        <Model3DGroup >
                            <!-- Lights -->
                            <AmbientLight Color="Gray"  />
                            <DirectionalLight Color="Wheat" Direction="-1,0,0" >
                                <DirectionalLight.Transform>
                                    <TranslateTransform3D OffsetX="100" />
                                </DirectionalLight.Transform>
                            </DirectionalLight>
                            <!--<DirectionalLight Color="Gray" Direction="-1,2,3" />-->

                            <GeometryModel3D Geometry="{ StaticResource LEDGeometry}">
                                <GeometryModel3D.Material>
                                    <DiffuseMaterial Brush="{ Binding DataContext.Cube.LEDs[0].LEDColor, ElementName = rootItem}" />
                                </GeometryModel3D.Material>
                                <GeometryModel3D.Transform>
                                    <TranslateTransform3D
                  OffsetX="{ Binding DataContext.Cube.LEDs[0].OffsetX, ElementName = rootItem}"
                  OffsetY="{ Binding DataContext.Cube.LEDs[0].OffsetY, ElementName = rootItem}"
                  OffsetZ="{ Binding DataContext.Cube.LEDs[0].OffsetZ, ElementName = rootItem}"
          />
                                </GeometryModel3D.Transform>
                            </GeometryModel3D>
